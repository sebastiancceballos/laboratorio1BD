              "path": ["personas"],
              "query": [ { "key": "skip", "value": "0" }, { "key": "limit", "value": "100" } ]
            }
          }
        },
        {
          "name": "Get Persona by ID",
          "request": {
            "method": "GET",
            "header": [],
