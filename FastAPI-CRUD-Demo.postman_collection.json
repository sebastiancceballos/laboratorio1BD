              "mode": "raw",
              "raw": "{\n  \"email\": \"juan.perez2@example.com\",\n  \"notes\": \"Actualizado\"\n}"
            },
            "url": { "raw": "{{base_url}}/personas/{{persona_id}}", "host": ["{{base_url}}"], "path": ["personas", "{{persona_id}}"] }
          }
        },
        {
          "name": "Delete Persona",
          "request": {
            "method": "DELETE",
            "header": [],
            "url": { "raw": "{{base_url}}/personas/{{persona_id}}", "host": ["{{base_url}}"], "path": ["personas", "{{persona_id}}"] }
          }
        }
      ]
    }
  ]
